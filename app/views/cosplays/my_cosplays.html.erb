
<div class="animate-charcter">
  <h1>My Cosplays</h1>
</div>
<div class="container">
  <div class="cards">
    <% @my_cosplays.each do |cosplay| %>
      <div class="card card-grid">
      <div class="cosplay-image">
        <%= cl_image_tag cosplay.photo.key %>
      </div>
        <div class="card-body">
          <h4 class="card-title"><%= cosplay.name  %></h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
